      "dimensions": {
        "pace": "0 = very slow & deliberate, 1 = very fast-paced",
        "structure": "0 = highly exploratory / tangent-friendly, 1 = strictly linear / step-by-step",
        "abstraction": "0 = very concrete & hands-on, 1 = highly abstract & theoretical",
        "interactivity": "0 = pure lecture / monologue, 1 = highly interactive (questions, exercises, polls)",
        "visual_dependency": "0 = text/words only, 1 = heavy visuals, diagrams, animations",
        "verbal_density": "0 = sparse minimal explanation, 1 = very wordy detailed narration",
        "repetition_need": "0 = says things once and moves on, 1 = revisits and reinforces constantly",
        "formality": "0 = very casual & conversational, 1 = very formal & academic",
        "humor_receptivity": "0 = no humor / dry, 1 = very funny / meme-heavy",
        "feedback_style": "0 = blunt / minimal feedback, 1 = warm detailed encouraging feedback",
        "autonomy": "0 = highly guided / hand-holds student, 1 = pushes student independence strongly",
        "cognitive_load_tolerance": "0 = low load sessions (easy spaced out), 1 = very high load (intense packed)",
        "attention_span": "0 = short bursts / chunked content, 1 = long uninterrupted deep dives",
        "motivation_type": "0 = extrinsic (grades deadlines), 1 = intrinsic (curiosity mastery)",
        "error_tolerance": "0 = very low (precision expected), 1 = very high (mistakes welcomed)",
        "social_preference": "0 = solo learning, 1 = highly collaborative / group-oriented",
        "real_world_need": "0 = pure theory, 1 = always anchors to real-world applications",
        "emotional_sensitivity": "0 = emotionally neutral / detached, 1 = highly empathetic & emotionally aware",
        "question_comfort": "0 = discourages questions / moves fast, 1 = actively invites all questions",
        "note_taking_style": "0 = minimal structure (free-form), 1 = highly structured (outlines headers)",
        "challenge_preference": "0 = comfort-zone content / supportive, 1 = pushes hard stretch goals",
        "context_need": "0 = jumps straight into content, 1 = always sets rich context/background first",
        "storytelling_affinity": "0 = facts-only / no narrative, 1 = heavy storytelling & anecdote-driven",
        "revision_style": "0 = no recap / assumed retention, 1 = frequent structured revision"
      }
    },
